<view id="judLawyer">
  <form class="needbox" bindsubmit="formSubmit">
      <view class="line">
        <view class="needone bordertop">
          <view>
            <text>*</text>
            <text>事件标题：</text>
          </view>
          <input type="text" class="inp" name="title" placeholder="请输入标题" />
          <view class="iconfont icon-you"></view>
        </view>
        <view class="needone bordertop">
          <view>
            <text>*</text>
            <text>事件类型：</text>
          </view>
          <picker bindchange="xlChange" mode="selector"  range="{{list}}" range-key="dictItemName">
            <input placeholder="请选择" value="{{mailType.dictItemName}}" class="piciput"></input>
          </picker>
          <view class="iconfont icon-you"></view>
        </view>
