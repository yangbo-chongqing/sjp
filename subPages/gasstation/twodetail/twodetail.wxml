<view class="activityTitle">
  {{detail.things_title}}
</view>
<view class="activitTop">
  <!-- <image src="{{detail.userPic}}" role="img"></image> -->
  <view class="createInfo">
    <!-- <text class="createName">{{detail.name}}</text> -->
