/* subPages/servelog/servelog.wxss */
page{
  background:#f5f5f5;
}
.logBox{
  display: flex;
  justify-content: flex-start;
  padding:30rpx;
  border-top:15rpx solid #f5f5f5;
  background: #fff;
  
}
.logLeft{
  flex: 1;
}
.logLeft>view{
  font-size:28rpx;
  color:#454545;
  line-height: 40rpx;
}
.logLeft>view>text:first-child{
  color:#8A8A8A;
}
.logRight{
  width:100rpx;
  height:100rpx;
  display: block;
}