/* subPages/social/social.wxss */
page{
  background-color: #f5f5f5;
}
.social-title{
  background-color: #fff;
}
.social-title .picker,
.social-title .picker>text{
  font-size: 32rpx;
  color: #444;
}
.social-title .picker{
  padding: 20rpx 0 20rpx 40rpx;
}
age{
  background: #f5f5f5;
}
.positioning{
  width: 100%;
  margin-top: 8rpx;
}
.positioning .posibox{
  width: 100%;
  background: #ffffff;
  box-sizing: border-box;
}
.toporg{
  margin-bottom: 32rpx;
  padding: 36rpx 36rpx 0rpx 36rpx;
}
.toporg text:nth-child(1){
  margin-right: 12rpx;
}
.toporg .textolor{
  font-size: 28rpx;
  font-weight: 600;
  color: #D33333;
}
.oneorg .textporg{
  padding: 0 30rpx;
  box-sizing: border-box;
}
.toporg>image{
  width: 36rpx;
  height: 36rpx;
  vertical-align: sub;
  margin-right: 10rpx;
}
.oneorg{
  font-size: 28rpx;
  color: #444444;
}
.posibox .oneorg .topboder{
  border-bottom: 2rpx solid #f5f5f5; 
  padding-bottom: 12rpx;
  margin-bottom: 0;
}
.positioning .onebox{
  background: #ffffff;
}
.report{
background: #FFFFFF;
}
.report .lis{
  width: 100%;
  padding: 40rpx 30rpx 22rpx 30rpx;
  display: flex;
  justify-content: left;
  border-bottom: 2rpx solid #f5f5f5;
}
.report .lis .imagebox{
  width: 220rpx;
  height: 216rpx;
  overflow: hidden;
  display: flex;
  align-items: center;
}
.report .lis .imagebox>image{
  width: 100%;
  height: 100%;
  border-radius: 4rpx;
}
.report .lis .textbox{
  width: 446rpx;
  margin-left: 24rpx;
}
.report .lis .textbox .text-view{
  margin-bottom: 23.8rpx;
}
.report .lis .textbox .text-view text{
  font-size: 36rpx;
  color: #444444;
  display: block;
}
.report .lis .textbox .text-vew text{
  font-size: 28rpx;
  color: #8A8A8A;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  letter-spacing:5rpx;
}
.report .lis .textsbox{
  width: 100%;
  display: flex;
  justify-content:space-between;
  padding: 24rpx 0 0 0;
}
.report .lis .textsbox text:nth-child(1){
  color: #8A8A8A;
  font-size: 24rpx;
  display: inline-block;
  margin-bottom: 23.8rpx;
}
.report .lis .textsbox text:nth-child(2){
  color: #8A8A8A;
  font-size: 24rpx;
}