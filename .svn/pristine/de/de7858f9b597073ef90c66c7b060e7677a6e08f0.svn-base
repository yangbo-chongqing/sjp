/* pages/hongyan/hongyan.wxss */
.container{
  padding: 0;
}
.container-header{
  height: 93px;
  width: 106px;
  background: #fff;
  text-align: center;
  display:flex;
  justify-content:center;
  align-items:center;
  border-top: 1px solid #f5f5f5;
  border-right: 1px solid #f5f5f5;
}
.container-header .header-news>view,
.container-header .header-photo>view,
.container-header .header-equa>view,
.container-header .header-user>view{
  width: 44px;
  height: 44px;
  background: linear-gradient(180deg, #FB936A 0%, #F44D3A 100%);
  box-shadow: 0px 2px 2px rgba(254, 122, 72, 0.56);
  border-radius: 5px;
  margin: auto;
}
.container-header .header-photo>view{
  background: linear-gradient(180deg, #C56FF7 0%, #855CCF 100%);
  box-shadow: 0px 2px 2px rgba(139, 93, 211, 0.56);
}
.container-header .header-equa>view{
  background: linear-gradient(180deg, #FFBC62 0%, #FE944C 100%);
  box-shadow: 0px 2px 2px rgba(253, 148, 75, 0.56);
}
.container-header .header-user>view{
  background: linear-gradient(180deg, #FFA791 0%, #FC857F 100%);
  box-shadow: 0px 2px 2px rgba(252, 134, 127, 0.56);
}
.container-header .header-news view>image,
.container-header .header-photo view>image,
.container-header .header-equa view>image,
.container-header .header-user view>image{
  width: 18px;
  height: 22px;
  margin-top: 10px;
}
.container-header .header-photo view>image{
  width: 25.94px;
  height: 21.65px;
}
.container-header .header-equa view>image{
  width: 25.45px;
  height: 25.72px;
}
.container-header .header-user view>image{
  width: 20.85px;
  height: 25.82px;
}
.container-header text{
  font-family: Noto Sans CJK;
  font-size: 14px;
  color: #48484A;
  line-height: 21px;
  margin-top: 12px;
}

/* 第二部分 */
.member{
  background: #fff;
  margin-top: 7px;
}
.line{
  margin-left: 16px;
  width: 100%;
  height: 1px;
  background: #f5f5f5;
}
.member .member-top,
.scale-top{
  padding: 11px 0 12px 12px;
}
.member .member-top image,
.scale-top image{
  width: 18px;
  height: 18px;
  margin-right: 8px;
  vertical-align: middle;
}
.member .member-top text,
.scale-top>view>text{
  color: #D33333;
  font-weight: bold;
  font-size: 14px;
}
.member .member-bottom{
  color: #999;
  font-size: 14px;
  display:flex;
  justify-content:center;
  align-items:center;
  text-align: center;
}
.member-bottom-left{
  display: inline-block;
  width: 49%;
}
.member-bottom-left view>image{
  width: 14px;
  height: 14px;
  vertical-align: middle;
  margin: 0 6px;
}
.member-bottom-left .member-stage{
  display: inline-block;
}
.member-bottom-left .member-stage text{
  color: #D33333;
  font-weight: bold;
}
.member-bottom-left>view:first-child,
.member-bottom-left>view:last-child{
  width: 123px;
  height: 22px;
  margin: auto;
  padding: 20px 0 12px 0;
}
.member-bottom-left>view:last-child{
  padding: 0 0 27px;
}
.len-line{
  width: 2px;
  height: 60px;
  background: #f5f5f5;
  display: inline-block;
}
.member-bottom-right{
  width: 49%;
  display: inline-block;
}
.member-bottom-right text{
  font-size: 16px;
  color: #999;
}
.member-bottom-right .yuan{
  display: inline-block;
}
.member-bottom-right .red,
.member-bottom-right .green{
  width: 21px;
  height: 21px;
  border-radius: 50%;
  display: inline-block;
}
.member-bottom-right .red{
  background: #D33333;
  box-shadow: 0px 0px 4px #D33333;
  vertical-align: middle;
  margin-left: 10px;
}
.member-bottom-right .green{
  background: #FFFFFF;
  border: 2px solid #36DA8B;
  vertical-align: middle;
  margin-left: 10px;
  box-sizing: border-box;
}

/* 第三部分 */
.scale{
  margin-top: 15px;
  background: #fff;
}
.scale-top>view{
  display: inline-block;
}
.scale-top>view image{
  width: 18px;
  height: 18px;
}
.scale-line{
  width: 2px;
  height: 14px;
  background: #DADADA;
  border-radius: 9px;
  margin: 0 8px;
  vertical-align: middle;
}
.scale-top>text{
  font-size: 10px;
  color: #999;
}
.scale-main>view{
  display:flex;
  justify-content:center;
  align-items:center;
}
.scale-main>view>.scale-main-text{
  width: 68%;
  color: #454545;
  font-size: 16px;
  display: inline-block;
  padding: 10px 12px 14px 15px;
}
.scale-main>view>.scale-main-text>text{
  color: #049DFB;
}
.scale-main-stage{
  display: inline-block;
}
.scale-main-stage image{
  width: 14px;
  height: 14px;
  margin-right: 8px;
  vertical-align: middle;
}
.scale-main-stage>text{
  font-size: 14px;
  color: #D33333;
}