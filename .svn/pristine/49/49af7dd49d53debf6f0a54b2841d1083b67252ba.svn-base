/**app.wxss**/
@import "./assets/css/iconfont.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.userpic {
  width: 100rpx;
  height: 100rpx;
  display: block;
  margin-right: 20rpx;
  border-radius: 50%;
}
.gold {
  width: 28rpx;
  height: 28rpx;
  display: block;
  margin-right: 20rpx;
}
