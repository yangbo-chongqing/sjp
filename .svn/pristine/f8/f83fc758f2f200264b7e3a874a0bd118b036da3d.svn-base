/* subPages/gridorganizationdetails/gridorganizationdetails.wxss */
.organization-details{
  border-top: 1px solid #f5f5f5;
}
.msg-title{
  margin: 24rpx 0 32rpx 28rpx;
}
.msg-title>image{
  width: 36rpx;
  height: 36rpx;
  margin-right: 6rpx;
  vertical-align: sub;
}
.msg-title>text{
  font-size: 28rpx;
  line-height: 42rpx;
  color: #d33333;
  font-weight: 600;
}
.msg-main-list{
  font-size: 28rpx;
  color: #999;
  margin: 0 0 14rpx 34rpx;
}
.msg-main-list>.name{
  color: #444;
  margin-left: 20rpx;
}
.tel-main-list{
  width: 690rpx;
  height: 220rpx;
  padding-left: 30rpx;
  margin: 0 auto 16rpx auto;
  border: 1px solid transparent;
  box-shadow: 0px 0px 4px rgba(0, 0, 0, 0.1);
  border-radius: 12rpx;
}
.tel-main-list-left{
  display: inline-block;
}
.tel-main-list-left>text,
.tel-main-list-left>view{
  font-size: 28rpx;
  color: #444;
  margin-top: 32rpx;
}
.tel-main-list-left>view>text:last-child{
  color: #d33333;
  margin-left: 22rpx;
}

.tel-main-list-left>view:last-child{
  margin-bottom: 40rpx;
}
.tel-main-list>.iconfont{
  width: 48rpx;
  height: 48rpx;
  float: right;
  margin-top: 96rpx;
  margin-right: 30rpx;
}