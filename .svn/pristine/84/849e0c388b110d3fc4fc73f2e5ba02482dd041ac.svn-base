/* subPages/forum/community/community.wxss */
page{
  background: #f5f5f5;
}
.community{
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing:border-box;
}
.combox{
  width: 100%;
  /* height: 544rpx; */
  background: #ffffff;
  border-radius: 8rpx;
  box-shadow: 0px 8rpx 8rpx rgba(255, 59, 48, 0.2); 
  padding: 32rpx 16rpx;
  margin-bottom: 20rpx;
  box-sizing: border-box;
}
.combox .comtext{
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-bottom: 30rpx;
}
.combox .comtext .aa{
  font-size: 32rpx;
  color: #444444; 
}
.combox .comtext .bb{
  color: #F26700;
  font-size: 24rpx;
}
.combox .comimg{
  width: 658rpx;
  height: 356rpx;
  overflow: hidden;
  display: flex;
  align-items: center;
}
.combox .comimg image{
  width: 100%;
}
.combox .comlogo{
  width: 50rpx;
  height: 50rpx;
  overflow: hidden;
  display: flex;
  align-items: center;
}
.combox .comlogo image{
  width: 100%;
}
.comboxs{
  display: flex;
  flex-wrap: nowrap;
  padding: 36rpx 0 0 14rpx;
  align-items: center;
}
.comboxs .twotext{
  margin-left: 22rpx;
  font-size: 28rpx;
  color: #444444;
}