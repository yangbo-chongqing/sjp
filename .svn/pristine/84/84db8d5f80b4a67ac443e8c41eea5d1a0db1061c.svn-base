/* subPages/manservice/manservice.wxss */
page{
  background: #f5f5f5;
  height:100%;
}
.line{
  width:100%;
  height:4rpx;
  background: transparent;
}
.servicebox{
  height:380rpx;
  background: #fff;
  margin-bottom:10rpx;
}
.serviceitem{
  margin-left: calc(30/10.8*1vh);
  box-sizing: border-box;
  
}
.servicetitle{
  display: flex;
  justify-content: space-between;
  height:88rpx;
  align-items: center;
  font-size:32rpx;
  border-bottom: 2rpx solid #f5f5f5;
  padding: 0 30rpx 0 0;
  box-sizing: border-box;

}

.servecontent{
  display: flex;
  justify-content: flex-start;
  box-sizing: border-box;
  padding:0 30rpx;
  align-items: center;
  height:160rpx;

}
.servecenter{
  flex: 1;
}
.serveright{
  display: flex;
  justify-content: flex-start;
  margin-top:10rpx;
  align-items: center;
}
.serveright>text{
  color:#d33333;
  font-size: 28rpx;
}
.hold{
  font-size:28rpx;
  color:#d33333;
}
.username{
  font-size:32rpx;
  color:#000;
}
.username>text{
  font-size:28rpx;
  color:#999;
}
.username>view:first-child,
.username>view:nth-child(2){
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.username>view:nth-child(2){
  margin-top: 20rpx;
}
.username>view:nth-child(2)>text{
  color: #8a8a8a;
  font-size: 28rpx;
}
.username>view:nth-child(2)>.iconfont{
  font-size: 40rpx;
  color: #d33333;
}
.place>text:first-child{
  color:#d33333;
  margin-right:10rpx;
  font-size:32rpx;


}
.place>text:last-child{
  color:#999;
  font-size:24rpx;
}
.servebottom{
  padding:0 30rpx;
  box-sizing: border-box;
  display: flex;
  flex: 1;
  justify-content: space-between;
  align-items: center;
}
.bottomleft{
  font-size:28rpx;
  color:#AEAEB2;
  
}

.bottomleft>view:last-child{
  margin-top:20rpx;
}
.bottomleft>view>text{
  color:#333;
}
.bottomright{
  width: 182rpx;
height: 60rpx;
box-sizing: border-box;
border-radius: 10rpx;
font-size:28rpx;
line-height: 60rpx;
text-align: center;
}
.color1{
  border: 2rpx solid #DADADA;
  color:#DADADA;

}
.color2{
  border: 2rpx solid #64D2FF;
  color:#64D2FF;

}
.color3{
  border: 2rpx solid  #FF9F0A;
  color: #FF9F0A;

}
.statuscolor1{
  color:#d33333;
  font-size:30rpx;

}
.statuscolor2{
  color:#DADADA;
  font-size:30rpx;
}
.statuscolor3{
  color:#64D2FF;
  font-size:30rpx;

}
.statuscolor4{
  color:#FF9F0A;
  font-size:30rpx;

}
.empty{
  text-align: center;
  height: 100%;
  line-height: 100vh;
}

.lookcode {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.15);
  position: fixed;
  top: 0;
  left: 0;
}

.lookcontent {
  position: absolute;
  width: 530rpx;
  height: 480rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  background: linear-gradient(0deg, #fff, #fff), #c4c4c4;
  border-radius: 28rpx;
  box-sizing: border-box;
  padding: 50rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.looktitle {
  font-size: 32rpx;
  color: #636366;
}

.looknum {
  width: 420rpx;
  height: 156rpx;
  background: rgba(255, 59, 48, 0.04);
  line-height: 156rpx;
  color: #d33333;
  font-weight: bold;
  font-size: 40rpx;
  text-align: center;
  letter-spacing: 20rpx;
}

.lookcomfire {
  width: 420rpx;
  height: 80rpx;
  margin: 0 auto;
  background: #d33333;
  border-radius: 14rpx;
  color:#fff;
  line-height: 80rpx;
  text-align: center;
}
.lookstart>text{
  font-size:40rpx;
  margin:0 10rpx;
}
.lookstart>text:last-child{
  font-size:32rpx;
  color:#FF9F0A;
}

.active{
  color:#FF9F0A;

}