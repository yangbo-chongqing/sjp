/* subPages/gridposition/gridposition.wxss */
/* 地图 */
.maps {
    height: 100vh;
    width: 100vw;
}

/* 信息 */
.msg {
    width: 100%;
    height: 120rpx;
    position: fixed;
    background-color: #fff;
    bottom: 0;
    left: 0;
    box-sizing: border-box;
    padding: 20rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.msg>.title {
    display: flex;
    flex-direction: column;
}

.msg>.title>text:last-child {
    font-size: 26rpx;
    color: #ccc;
}

.msg>.logo {
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
}