/* subPages/personData/personData.wxss */
page{
  background: #f5f5f5;
}
.line{
  width:100%;
  height:2rpx;
  background: #f5f5f5;
}
.all{
  background: #fff;
}
.item{
  height:180rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  padding:0 30rpx 0 0;
  border-bottom: 2rpx solid #f5f5f5;
  background: #fff;


  margin-left:calc(30/10.8*1vh);
}
.userpicture{
  flex: 1;
  font-size:28rpx;
  color:#999;
   background: #fff;
}
.img>image{
  width:120rpx;
  height:120rpx;
  display: block;
  border-radius: 50%;
  margin-right:30rpx;
}
.img>view>image{
  width:120rpx;
  height:120rpx;
  display: block;
  border-radius: 50%;
  margin-right:30rpx;
}
.right{
  font-size:40rpx;
  color:#999;
}
.itemOther{
   margin-left:calc(30/10.8*1vh);
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding:0 30rpx 0 0;
  border-bottom: 2rpx solid #f5f5f5;
  height:90rpx;
   background: #fff;
}
.username{
  margin-right:30rpx;
  font-size:28rpx;
  color:#999;
}
.itemOther>input{
  text-align: right;
  flex: 1;
  font-size:28rpx;
}
.itemOther>picker{
  text-align: right;
  flex: 1;
  font-size:28rpx;
}
.code{
  color:#999;
}
.codes{
  width:100%;
  height:100%;
  background: rgba(0, 0, 0, 0.15);
  position: fixed;
  top:0;
  left:0;
}
.codecontent{
  width:504rpx;
  height:600rpx;
  position: absolute;
  top:45%;
  left:50%;
  transform: translate(-50%,-50%);
  display: flex;
  justify-content: space-between;
  flex-direction: column;
  align-items: center;

}
.codecontent>image{
  width:504rpx;
  height:504rpx;
  display: block;
}
.det{
  font-size:60rpx;

  color:#fff;
}
.save{
  width: 516rpx;
  height: 88rpx;
  margin: 80rpx auto 0;
  /* background: #E5E5EA; */
  color: #fff;
  font-size: 32rpx;
  line-height: 88rpx;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;

}
.backred{
  background: #d33333;

}
.backfff{
  background: #E5E5EA;

}
button:not([size='mini']){
  width:70%;
}
button{
  font-size: 32rpx;
  padding: 0;
  border-radius: none;
}