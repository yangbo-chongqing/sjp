/* components/joinsuccess/joinsuccess.wxss */
@import "/assets/css/iconfont.wxss";
.model{
  width:100%;
  height:100%;
  position: fixed;
  top:0;
  left:0;
  background: rgba(31, 31, 31, 0.42);

}
.modelContent{
  width:624rpx;
  height:628rpx;
  background: #fff;
  position: absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
  border-radius: 28rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  padding:100rpx 0;
  box-sizing: border-box;
  
}
.font{
  font-size:220rpx;
  color:#D33333;
}
.successname{
  font-size:32rpx;
  color:#D33333;
  margin-bottom:80rpx;
}
.closebtn{
  width:420rpx;
  height:80rpx;
  background: #D33333;
  color:#fff;
  border-radius: 14rpx;
  font-size: 32rpx;
  line-height: 80rpx;
  text-align: center;

}